<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pilar.quimica.quimicapilar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_quimicapilar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_quimicapilar_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.CadastroActivity" />
        <activity android:name=".Activity.ResetActivity" />
        <activity
            android:name=".Activity.HistoricoActivity"
            android:label="@string/title_activity_historico"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>